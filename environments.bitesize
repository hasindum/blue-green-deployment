project: spring-tst
environments:
  - name: dev
    namespace: spring-tst-dev
    deployment:
      method: bluegreen
      mode: auto
      active: blue
    services:
      - name: spring-app
        application: nginx-blue
        external_url: app-spring-tst-us1-dev.bite.pearsondev.tech
        port: 80
        replicas: 2
        backend_port: 80
        ssl: "false"
