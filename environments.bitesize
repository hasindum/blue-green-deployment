project: nginx-tst
environments:
  - name: dev
    namespace: nginx-tst-dev
    services:
      - name: nginx-tst-app
        application: nginx-blue
        version: 1.0.0
        external_url: nginx-tst-eu1-dev.bite.pearsondev.tech
        port: 80
        replicas: 2
        backend_port: 80
        ssl: "false"
        deployment:
          method: bluegreen
          active: green
